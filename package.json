{
  "name": "fiebre-web-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "icons": "npx @svgr/cli --out-dir app/components/svg -- public/icons",
    "svg": "npx @svgr/cli --out-dir app/components/svg -- svgr",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "dev": "remix dev",
    "dev:assets": "run-p dev:*"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/css-bundle": "^2.2.0",
    "@remix-run/node": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/serve": "^2.2.0",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@tanstack/react-table": "^8.12.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "front-matter": "^4.0.2",
    "isbot": "^3.6.8",
    "jotai": "^2.6.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.383.0",
    "marked": "^11.0.0",
    "mongoose": "^8.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-validated-form": "^5.1.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.6",
    "zod-form-data": "^2.0.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.38",
    "prettier": "3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
